        <!--输血检测技术 -->
        <record model="ir.actions.act_window" id="action_mqc_blood_tech">
            <field name="name">输血检测技术</field>
            <field name="res_model">mqc.blood.tech</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
