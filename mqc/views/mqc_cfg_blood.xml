        <!-- 表单视图 -->
        <record id="view_mqc_blood_tech_form" model="ir.ui.view">
            <field name="name">mqc.blood.tech.form</field>
            <field name="model">mqc.blood.tech</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="name"></field>
                    </group>
                </form>
            </field>
        </record>
        <!-- 列表视图 -->
